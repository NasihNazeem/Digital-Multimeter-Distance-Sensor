library IEEE;
USE IEEE.STD_LOGIC_1164.ALL;

ENTITY Error IS
	port(
		distance: 	in std_logic_vector(15 downto 0);
		errorout: 	out std_logic_vector(15 downto 0);
		s2      :   in std_logic
		);
	end Error;
	
architecture Behaviour OF Error is
begin
process(distance)
begin
		if (distance > "0011001100000000") then
			errorout <= "1010101010101010";
		else 
			errorout <= distance;
		end if;	
	 end process;
end Behaviour;

